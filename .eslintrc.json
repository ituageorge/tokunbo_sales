{
  "root": true,
  "env": {
    "node": true
  },
  "rules": {
    "no-unused-vars": "off",
    "react/prop-types": "off", // Disable prop-types rule
     "react/react-in-jsx-scope": "off"
    // other rules...
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    },
    "requireConfigFile": false
  },
  "plugins": [
    "react"
  ],
  "extends": [
    "next/core-web-vitals",
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:@next/next/recommended"
  ],
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
